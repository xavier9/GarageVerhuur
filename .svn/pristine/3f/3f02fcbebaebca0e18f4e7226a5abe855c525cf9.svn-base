     *
     * @param gemente Value to set for property 'gemente'.
     */
    public void setGemente(String gemente) {
        this.gemente = gemente;
    }

    /**
     * Getter for property 'land'.
     *
     * @return Value for property 'land'.
     */
    public String getLand() {
        return land;
    }

    /**
     * Setter for property 'land'.
     *
     * @param land Value to set for property 'land'.
     */
    public void setLand(String land) {
        this.land = land;
    }


    /** {@inheritDoc} */
    @Override
    public String toString() {
        return "Adres{" +
                "ID=" + ID +
                ", straat='" + straat + '\'' +
                ", nr=" + nr +
                ", bus='" + bus + '\'' +
                ", postcode='" + postcode + '\'' +
                ", gemente='" + gemente + '\'' +
                ", land='" + land + '\'' +
                '}';
    }
}
