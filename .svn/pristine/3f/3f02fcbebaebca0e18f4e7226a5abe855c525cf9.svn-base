package com.ehb.xavier.garageverhuur.classes;

import com.j256.ormlite.field.DatabaseField;


/**
 * The type Adres.
 */
public class Adres {
    @DatabaseField(generatedId = true, allowGeneratedIdInsert = true)
    private int ID;
    @DatabaseField
    private String straat;
    @DatabaseField
    private int nr;
    @DatabaseField
    private String bus;
    @DatabaseField
    private String postcode;
    @DatabaseField
    private String gemente;
    @DatabaseField
    private String land;

    /** Constructs a new Adres. */
    public Adres() {
    }

    /**
     * Instantiates a new Adres.
     *
     * @param ID the iD
     */
    public Adres(int ID) {
        this.ID = ID;
    }

    /**
     * Instantiates a new Adres.
     *
     * @param straat the straat
     * @param bus the bus
     * @param gemente the gemente
     * @param land the land
     * @param postcode the postcode
     * @param nr the nr
     */
    public Adres(String straat, String bus, String gemente, String land, String postcode, int nr) {
        this.straat = straat;
        this.nr = nr;
        this.bus = bus;
        this.gemente = gemente;
        this.postcode = postcode;
        this.land = land;
    }

    /**
     * Instantiates a new Adres.
     *
     * @param straat the straat
     * @param bus the bus
     * @param gemente the gemente
     * @param land the land
     * @param postcode the postcode
     * @param nr the nr
     * @param ID the iD
     */
    public Adres(String straat, String bus, String gemente, String land, String postcode, int nr, int ID) {
        this.ID = ID;
        this.straat = straat;
        this.nr = nr;
        this.bus = bus;
        this.postcode = postcode;
        this.gemente = gemente;
        this.land = land;
    }

    /**
     * Getter for property 'postcode'.
     *
     * @return Value for property 'postcode'.
     */
    public String getPostcode() {
        return postcode;
    }

    /**
     * Setter for property 'postcode'.
     *
     * @param postcode Value to set for property 'postcode'.
     */
    public void setPostcode(String postcode) {
        this.postcode = postcode;
    }

    /**
     * Getter for property 'ID'.
     *
     * @return Value for property 'ID'.
     */
    public int getID() {
        return ID;
    }

    /**
     * Setter for property 'ID'.
     *
     * @param ID Value to set for property 'ID'.
     */
    public void setID(int ID) {
        this.ID = ID;
    }

    /**
     * Getter for property 'straat'.
     *
     * @return Value for property 'straat'.
     */
    public String getStraat() {
        return straat;
    }

    /**
     * Setter for property 'straat'.
     *
     * @param straat Value to set for property 'straat'.
     */
    public void setStraat(String straat) {
        this.straat = straat;
    }

    /**
     * Getter for property 'nr'.
     *
     * @return Value for property 'nr'.
     */
    public int getNr() {
        return nr;
    }

    /**
     * Setter for property 'nr'.
     *
     * @param nr Value to set for property 'nr'.
     */
    public void setNr(int nr) {
        this.nr = nr;
    }

    /**
     * Getter for property 'bus'.
     *
     * @return Value for property 'bus'.
     */
    public String getBus() {
        return bus;
    }

    /**
     * Setter for property 'bus'.
     *
